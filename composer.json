{
    "name": "has-to-be/factory-bot",
    "type": "library",
    "description": "Fixtures replacement with a straightforward definition syntax",
    "keywords": ["factory_bot","fixture","factory","bot", "testing"],
    "homepage": "https://has-to-be.com",
    "license": "BSD-3",
    "authors": [
        {
            "name": "Sebastian Huber",
            "email": "sebastian.huber@has-to-be.com",
            "homepage": "https://twitter.com/SebCHuber",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "php-parallel-lint/php-console-highlighter": "^0.4.0",
        "php-parallel-lint/php-parallel-lint": "^1.1",
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "FactoryBot\\": "FactoryBot/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FactoryBot\\Tests\\": "tests/"}
    },
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor",
            "phpunit tests/"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
